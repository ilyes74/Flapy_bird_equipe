        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flapy_bird"
        mc:Ignorable="d"
        Title="PageDemarrage1" Height="450" Width="800">
    <Window.Background>
        <ImageBrush x:Name="ImgCiel" ImageSource="/Images/Ciel + Herbes.png"/>
    </Window.Background>
    <ContentControl x:Name="ZoneDemarrage" >
        <Image Margin="-1,141,-1,-62" Source="/Images/Herbes.png" Stretch="Fill"/>
    </ContentControl>
</Window>